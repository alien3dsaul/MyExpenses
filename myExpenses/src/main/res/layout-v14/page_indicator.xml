<?xml version="1.0" encoding="utf-8"?>
<com.rd.PageIndicatorView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:attrs="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_centerInParent="true"
    android:id="@+id/pageIndicatorView"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    attrs:piv_animationType="slide"
    attrs:piv_interactiveAnimation="true"
    attrs:piv_viewPager="@id/viewpager"
    attrs:piv_selectedColor="?android:textColorPrimary"
    attrs:piv_unselectedColor="?android:textColorSecondary"
    tools:showIn="@layout/onboarding"/>